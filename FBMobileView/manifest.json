{
  "manifest_version": 2,
  "name": "Facebook Mobile View",
  "description": "Open Mobile View of facebook in your browser",
  "version": "1.0",
  "background" : {
    "scripts":["background.js"]

  },
  "icons":{
    "16": "icon.png"
  },
  "browser_action": {
    "default_icon": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
    },
    "default_title": "Facebook Mobile"
  },
  "permissions": [
    "tabs"
]
}
