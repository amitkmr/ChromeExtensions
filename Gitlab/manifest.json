{
  "manifest_version": 2,
  "name": "Gitlab",
  "description": " Gitlab view",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_title": "Gitlab"
  },
  "permissions": [
    "tabs"
  ]
}